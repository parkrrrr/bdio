# Display definition for HumanWare : Brailliant B 80

# There are four thumb keys on the front. From left to right, they are:
# * Previous, Back, Advance, Next.

ttydriver    "HumanWare"
ttymodel     "B80"
ttycode      "hw"
manufacturer "HumanWare"
model        "Brailliant B 80"

display {
  row {
    cells 8;
    router1 {
      router   1;  # RoutingKey
    }
  }

  button 1   0 17 "Previous"; 
  button 2   0 18 "Back"; 
  button 3   0 19 "Advance"; 
  button 4   0 20 "Next"; 
  button 5   0 11 "Display1";   # brltty has no binding for this key
  button 6   0 12 "Display2";   # brltty has no binding for this key
  button 7   0 13 "Display3";   # brltty has no binding for this key
  button 8   0 14 "Display4";   # brltty has no binding for this key
  button 9   0 15 "Display5";   # brltty has no binding for this key
  button 10   0 16 "Display6";   # brltty has no binding for this key
}
