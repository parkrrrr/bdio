# Display definition for HIMS : SyncBraille


ttydriver    "HIMS"
ttymodel     "sync"
ttycode      "hm"
manufacturer "HIMS"
model        "SyncBraille"

display {
  row {
    cells 8;
    router1 {
      router   1;  # RoutingKey
    }
  }

  left {
    button 1   0 12 "LeftScrollUp"; 
    button 2   0 15 "LeftScrollDown"; 
  }

  right {
    button 3   0 13 "RightScrollUp"; 
    button 4   0 14 "RightScrollDown"; 
  }

}
