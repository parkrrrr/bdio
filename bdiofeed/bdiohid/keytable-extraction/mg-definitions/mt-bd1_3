# Display definition for Metec : BD I (with 3 keys)


ttydriver    "Metec"
ttymodel     "bd1_3"
ttycode      "mt"
manufacturer "Metec"
model        "BD I (with 3 keys)"

display {
  row {
    cells 8;
    router1 {
      router   1;  # RoutingKey
    }
  }

  button 1   0 6 "Up"; 
  button 2   0 4 "Select"; 
  button 3   0 2 "Down"; 
}
