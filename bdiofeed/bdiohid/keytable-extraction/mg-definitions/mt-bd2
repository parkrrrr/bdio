# Display definition for Metec : BD II


ttydriver    "Metec"
ttymodel     "bd2"
ttycode      "mt"
manufacturer "Metec"
model        "BD II"

display {
  row {
    cells 8;
    router1 {
      router   1;  # RoutingKey
    }
  }

  left {
    button 1   0 6 "LeftUp";   # brltty has no binding for this key
    button 2   0 2 "LeftDown";   # brltty has no binding for this key
  }

  right {
    button 3   0 3 "RightUp";   # brltty has no binding for this key
    button 4   0 0 "RightDown";   # brltty has no binding for this key
  }

}
