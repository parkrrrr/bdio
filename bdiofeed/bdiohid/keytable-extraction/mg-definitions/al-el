  button 2   0 65 "Control";   # brltty has no binding for this key
  jenter    0 46; # JoystickEnter   # brltty has no binding for this key
  jleft    0 45; # JoystickLeft   # brltty has no binding for this key
  jright    0 49; # JoystickRight   # brltty has no binding for this key
