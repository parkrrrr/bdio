# Display definition for Voyager : Braille Pen 12


ttydriver    "Voyager"
ttymodel     "bp"
ttycode      "vo"
manufacturer "Voyager"
model        "Braille Pen 12"

display {
  row {
    cells 8;
    router1 {
      router   1;  # RoutingKey
    }
  }

  dot1   0 0; # Dot1   # brltty has no binding for this key
  dot2   0 1; # Dot2   # brltty has no binding for this key
  dot3   0 2; # Dot3   # brltty has no binding for this key
  dot4   0 3; # Dot4   # brltty has no binding for this key
  dot5   0 4; # Dot5   # brltty has no binding for this key
  dot6   0 5; # Dot6   # brltty has no binding for this key
  button 1   0 6 "Shift";   # brltty has no binding for this key
  space   0 14; # Space   # brltty has no binding for this key
  button 2   0 7 "Control";   # brltty has no binding for this key
  jenter    0 9; # JoystickEnter   # brltty has no binding for this key
  jleft    0 10; # JoystickLeft   # brltty has no binding for this key
  jright    0 13; # JoystickRight   # brltty has no binding for this key
  jup    0 11; # JoystickUp   # brltty has no binding for this key
  jdown    0 12; # JoystickDown   # brltty has no binding for this key
  button 3   0 8 "ScrollLeft";   # brltty has no binding for this key
  button 4   0 15 "ScrollRight";   # brltty has no binding for this key
}
