# Display definition for Baum : (variokeys=yes)


ttydriver    "Baum"
ttymodel     "vk"
ttycode      "bm"
manufacturer "Baum"
model        "(variokeys=yes)"

display {
  row {
    cells 8;
    router1 {
      router   1;  # RoutingKey
    }
  }

  button 1   (0, 5) "Display6"; 
  button 2   (0, 4) "Display5"; 
  button 3   (0, 3) "Display4"; 
  button 4   (0, 2) "Display3"; 
  button 5   (0, 1) "Display2"; 
  button 6   (0, 0) "Display1"; 
}
